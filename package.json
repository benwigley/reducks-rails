{
  "name": "reducks-rails",
  "version": "1.0.0",
  "description": "Hook up Redux to Rails using the Reducks method",
  "main": "dist/reducks-rails.js",
  "scripts": {
    "start": "webpack --progress --watch --config webpack.config.js",
    "build": "npm test && webpack --progress --config webpack.config.js",
    "test": "jest --coverage --silent",
    "test:watch": "npm test -- --watch --bail"
  },
  "keywords": [
    "redux",
    "reducks",
    "rest",
    "api",
    "rails",
    "redux-rails"
  ],
  "author": "Ben Wigley (http://github.com/benwigley)",
  "license": "ISC",
  "peerDependencies": {
    "redux": ">=3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "axios": "^0.20.0",
    "babel-loader": "^8.1.0",
    "humps": "^2.0.1",
    "jest": "^26.4.2",
    "lodash": "^4.17.20",
    "nock": "^13.0.4",
    "path": "^0.12.7",
    "pluralize": "^8.0.0",
    "redux": "^4.0.5",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "testRegex": "(/test/.*\\.spec.js)$",
    "collectCoverageFrom": [
      "src/**"
    ]
  }
}
